{
  "name": "lockd.app",
  "version": "1.0.0",
  "description": "Blockchain transaction scanner for Lockd.app",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles",
    "start": "node dist/index.js",
    "scanner": "tsx src/scripts/startScanner.ts",
    "scanner:cleanup": "CLEANUP_DB=true tsx src/scripts/startScanner.ts",
    "generate-tags": "tsx scripts/generateTags.js",
    "download-model": "node scripts/download-model.js",
    "verify-tag-system": "node scripts/verify-tag-system.js",
    "tag-generator": "tsx src/scripts/testTagGenerationAPI.ts",
    "fetch-bsv-price": "tsx scripts/bsv-price-fetcher.ts",
    "update-stats-price": "tsx scripts/update-stats-bsv-price.ts",
    "setup-bsv-price-cron": "bash scripts/setup-bsv-price-cron.sh",
    "dev": "USE_DIRECT_URL=true concurrently \"vite --port 3000 --strictPort\" \"PORT=3003 tsx watch src/server.ts\"",
    "test:parser": "NODE_ENV=test jest --forceExit --silent src/services/tests/parser.integration.test.ts",
    "test:parser:manual": "tsx src/scripts/test-tx-parser.ts",
    "test:db": "tsx src/scripts/test-db-save.ts",
    "view:db": "tsx src/scripts/view-saved-tx.ts",
    "prisma:sync": "tsx scripts/sync-prisma-schema.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:validate": "prisma validate"
  },
  "dependencies": {
    "@bsv/sdk": "^1.3.36",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@gorillapool/js-junglebus": "^0.4.4",
    "@mui/material": "^6.4.6",
    "@prisma/client": "^5.22.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.6.7",
    "bmapjs": "^0.4.0-beta.63",
    "bsv": "^1.5.6",
    "chart.js": "^4.4.8",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "express": "^4.18.2",
    "file-type": "^20.4.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.12.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.13.3",
    "postgres": "^3.4.5",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.0.1",
    "react-images-uploading": "^3.1.7",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.15.1",
    "scrypt-ts": "^1.4.5",
    "winston": "^3.11.0",
    "yours-wallet-provider": "^3.6.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.5.12",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.11.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "prisma": "^5.9.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  }
}
