{
  "name": "lockd.app",
  "private": true,
  "version": "1.0.0",
  "description": "Blockchain transaction scanner for MAP protocol",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch src/server.ts\"",
    "start": "tsx src/server.ts",
    "scanner": "tsx src/services/scanner/unifiedScanner.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "prisma generate",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch"
  },
